<aspects>
  <aspect name="Genes">
    <subtitle>Genome annotation</subtitle>
    <icon-image>model/images/genomics.gif</icon-image>
    <large-image>model/images/genomics.gif</large-image>
    <intro-text>
      The gene structure and other genome annotation in BovineMine are provided by
      a variety of curated source databases.  In most cases BovineMine includes
      all genome features that have chromosomal locations.
    </intro-text>
    <tile-name>model/genomics.jsp</tile-name>
    <aspect-source name="BGD" url="http://www.bovinegenome.org"/>
    <aspect-source name="NCBI" url="http://www.ncbi.nlm.nih.gov"/>
    <aspect-source name="Ensembl" url="http://www.ensembl.org/index.html"/>
  </aspect>

  <aspect name="Transcripts">
    <subtitle>Transcripts</subtitle>
    <icon-image>model/images/genomics.gif</icon-image>
